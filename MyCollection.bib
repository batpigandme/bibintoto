journal = {Aphasiology},
keywords = {1976,and between sentences,coherence,cohesion,cohesive ties are a,declarative memory,defined as surface indicators,discourse,halliday,hasan,hippocampus,linguistic device that gives,of the relations within,our communication continuity},
