keywords = {STS,hermeneutics},
mendeley-tags = {STS,hermeneutics},
number = {2},
pages = {1--33},
pmid = {811138867},
title = {{The Theory of Reflexive Modernization}},
url = {http://pubs.acs.org/doi/abs/10.1021/ac085995z},
volume = {20},
year = {2003}
}
