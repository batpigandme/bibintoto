journal = {Cognition},
keywords = {Bayesian,Causal model,Dual-process,Empathy,Evaluation,Model-free and model-based learning and control,Moral development,Moral judgment,Reinforcement learning,Simulation,Trolley problem},
pmid = {27601269},
title = {{Moral learning: Why learning? Why moral? And why now?}},
url = {http://dx.doi.org/10.1016/j.cognition.2016.08.015},
year = {2016}
}
