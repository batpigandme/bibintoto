isbn = {0736-3761},
issn = {0736-3761},
journal = {Journal of Consumer Marketing},
keywords = {Brand integration,brand awareness,dual coding theory,second screen,sponsorship-linked marketing,sport advertising,sport sponsorship,televised sports broadcasts},
number = {2},
pages = {71--84},
title = {{The effects of second screen use on sponsor brand awareness: a dual coding theory perspective}},
url = {http://www.emeraldinsight.com/doi/abs/10.1108/JCM-02-2014-0861},
