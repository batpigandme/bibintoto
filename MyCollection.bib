keywords = {HTLWDV,STS,algorithms,anthropology,data science,epistemology,rationality,russia},
mendeley-tags = {HTLWDV,STS},
pages = {1--13},
title = {{Algorithmic rationality: Epistemology and efficiency in the data sciences}},
url = {http://journals.sagepub.com/doi/10.1177/2053951717700925},
year = {2017}
}
