@incollection{Cambre2017,
abstract = {It seemed to some of us that who you were, and where you were, determined to a very considerable extent what you were able to see as the facts.-Desmond Tutu, On the Soweto Uprising At the International Ethics Roundtable in 2014 on Information Ethics and Global Citizenship, Rafael Capurro spoke on citizenship in the digital age and called for a phenomenology of messages, not a hermeneutic phenomenology but a phenomenology of communication, to work on the level of the transmission of messages and decisions on their form that impact their interpretation. Scholars in communication studies (Langlois, 2011; Drucker 2011) and sociology of information (Knorr-Cetina, 2003) and others have been concerned with the meaning potential of codes, interfaces and other aspects of digitally mediated communication that produce the virtual containers forming messages, and thus also informing them. While some scholars focus on aspects of " transmissive control " (McKelvey, 2008) and how internet routing algorithms impact what can be seen and when by those communicating, others have focused on the level of the interfaces themselves asking what happens when the notion of " user " is substituted for that of a " subject " (Drucker, 2011, p.1). Capurro was concerned with what happens to democracy in the face of technicity (or the shift from modernism to what Manuel Castells famously called informationalism) and a situation where he sees the freedom of code superseding and perhaps impeding the freedom of information. In this context, the conversation about " information ethics " emerges; giving rise to questions about the role of education in the face of the increasingly urgent and complex needs for ethical information literacy education. While phenomenology of communication does exist under the rubric of communicology 1 – a postpositivist discipline that draws on phenomenology and 1 Lanigan, R. L. (1992). The human science of communicology: A phenomenology of discourse in Foucault and Merleau-Ponty. Pittsburgh, PA: Duquesne University Press. Cambre and Arshad-Ayaz 65 semiotics to study embodiment, consciousness and meaning-making as part of scholarly efforts to create space to fashion and position oneself otherwise – not enough scholarly attention has been given to this emergent area. Communicology has the potential to help scholars detect emerging aesthetics and cultural forms specific to an informatic society or what Lev Manovich (2005) has described as info-aesthetics. It goes without saying that the representation of information (form, design or containers of various sorts) is also information, and is not neutral. The scale and volume of information availability has driven the development of forms of data and information visualization. However, these areas, where many decisions are made with regarding to what exactly will be foregrounded and backgrounded, are populated by " designers, computer scientists, and artists " that, while helping us understand the data in certain ways, can also end up " simply anesthetizing it " (Manovich 2005, p.3). This last observation Manovich makes, alerts scholars to the need that Kimberli Kelmor (2014) justifies as a right to information meaning citizens are included in the oversight of how information is presented: But how are citizens prepared for these decisions? The field of education is slow to embrace changes that have already had transformative impact in other social sciences where the monolithic conception of modern scientific knowledge has been questioned in terms of its epistemological privilege in relation to other knowledges},
address = {Jefferson, NC},
author = {Cambre, Carolina and Arshad-Ayaz, Adeela},
booktitle = {Information Ethics, Globalization and Citizenship: Essays on Ideas to Praxis},
editor = {Samek, T and Shultz, L},
file = {:Users/maraaverick/Dropbox/Mendeley Desktop/Cambre, Arshad-Ayaz - 2017 - Literacies future past Inwardness as ethical information.pdf:pdf},
keywords = {EARL,HTLWDV,STS,hermeneutics},
mendeley-tags = {EARL,HTLWDV,STS,hermeneutics},
number = {April},
pages = {64--79},
publisher = {Macfarland Publishers},
title = {{Literacies future past: Inwardness as ethical information}},
url = {https://www.researchgate.net/publication/314240508{\_}Literacies{\_}future{\_}past{\_}Inwardness{\_}as{\_}ethical{\_}information},
year = {2017}
}
@article{Cameron2011,
abstract = {As the number of people in need of help increases, the degree of compassion people feel for them ironically tends to decrease. This phenomenon is termed the collapse of compassion. Some researchers have suggested that this effect happens because emotions are not triggered by aggregates. We provide evidence for an alternative account. People expect the needs of large groups to be potentially overwhelming, and, as a result, they engage in emotion regulation to prevent themselves from experiencing overwhelming levels of emotion. Because groups are more likely than individuals to elicit emotion regulation, people feel less for groups than for individuals. In Experiment 1, participants displayed the collapse of compassion only when they expected to be asked to donate money to the victims. This suggests that the effect is motivated by self-interest. Experiment 2 showed that the collapse of compassion emerged only for people who were skilled at emotion regulation. In Experiment 3, we manipulated emotion regulation. Participants who were told to down-regulate their emotions showed the collapse of compassion, but participants who were told to experience their emotions did not. We examined the time course of these effects using a dynamic rating to measure affective responses in real time. The time course data suggested that participants regulate emotion toward groups proactively, by preventing themselves from ever experiencing as much emotion toward groups as toward individuals. These findings provide initial evidence that motivated emotion regulation drives insensitivity to mass suffering.},
annote = {NULL},
author = {Cameron, C Daryl and Payne, B Keith},
doi = {10.1037/a0021643},
file = {:Users/maraaverick/Dropbox/Mendeley Desktop/Cameron, Payne - 2011 - Escaping affect how motivated emotion regulation creates insensitivity to mass suffering.pdf:pdf},
isbn = {0022-3514},
issn = {1939-1315},
journal = {Journal of personality and social psychology},
keywords = {Altruism,Disasters,Emotional Intelligence,Empathy,Female,Helping Behavior,Humans,Male,Models, Psychological,Motivation,Social Responsibility,Stress, Psychological,Stress, Psychological: psychology},
number = {1},
pages = {1--15},
pmid = {21219076},
title = {{Escaping affect: how motivated emotion regulation creates insensitivity to mass suffering.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21219076},
volume = {100},
year = {2011}
}
