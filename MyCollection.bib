editor = {Turner, Bryan S},
file = {:Users/maraaverick/Dropbox/Mendeley Desktop/Roosth, Silbey - 2009 - Science and Technology Studies From Controversies to Posthumanist Social Theory.pdf:pdf},
isbn = {9781444304992},
keywords = {Actor network theory (ANT),Biosociality,EARL,Earliest roots of science and technology studies (,Production of scientific knowledge - elusive bound,Public understanding of science (PUS),STS,STS studies of institutionalization,Science and Technology Studies - from controversie},
