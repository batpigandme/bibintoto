editor = {Hackett, Edward J. and Amsterdamska, Olga and Lynch, Michael and Wajcman, Judy},
file = {:Users/maraaverick/Dropbox/Mendeley Desktop/Burri, Dumit - 2008 - Social Studies of Scientific Imaging and Visualization.pdf:pdf},
isbn = {9780262083645 (hardcover alk. paper)$\backslash$r0262083647 (hardcover alk. paper)},
