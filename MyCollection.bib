isbn = {0140-525X (Print) 0140-525X (Linking)},
issn = {0140525X},
journal = {Behavioral and Brain Sciences},
keywords = {1,EARL,STS,attention,attention, enumeration, information chunks, memory,enumeration,information chunks,introduction to the problem,memory capacity,of mental,processing capacity,processing channels,serial recall,short-,storage capacity,term memory,verbal recall,working memory capacity},
