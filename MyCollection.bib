journal = {Journal of personality and social psychology},
keywords = {Adolescent,Adult,Cognition,Cognition: physiology,Decision Making,Decision Making: physiology,Empathy,Empathy: physiology,Ethical Theory,Female,Humans,Judgment,Judgment: physiology,Male,Morals,Religion and Psychology,Students,Students: psychology},
number = {2},
pages = {216--35},
pmid = {23276267},
title = {{Deontological and utilitarian inclinations in moral decision making: a process dissociation approach.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23276267},
volume = {104},
year = {2013}
}
