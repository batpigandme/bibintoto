keywords = {Artificial Intelligence (cs.AI),Computation and Language (cs.CL),Computer Vision and Pattern Recognition (cs.CV)},
month = {dec},
title = {{Attentive Explanations: Justifying Decisions and Pointing to the Evidence}},
