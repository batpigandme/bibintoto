issn = {1553734X},
journal = {PLoS Computational Biology},
number = {1},
pmid = {21283774},
title = {{Risk, unexpected uncertainty, and estimation uncertainty: Bayesian learning in unstable settings}},
volume = {7},
year = {2011}
}
