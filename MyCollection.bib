issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Achievement,Athletes,Athletic Performance,Basketball,Cluster Analysis,Entropy,Humans},
number = {11},
pmid = {23139744},
title = {{Basketball Teams as Strategic Networks}},
url = {http://dx.plos.org/10.1371/journal.pone.0047445},
volume = {7},
year = {2012}
