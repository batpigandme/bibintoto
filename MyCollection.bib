@article{Phillips2014,
abstract = {In choices between uncertain options, information search can increase the chances of distinguishing good from bad options. However, many choices are made in the presence of other choosers who may seize the better option while one is still engaged in search. How long do (and should) people search before choosing between uncertain options in the presence of such competition? To address this question, we introduce a new experimental paradigm called the competitive sampling game. We use both simulation and empirical data to compare search and choice between competitive and solitary environments. Simulation results show that minimal search is adaptive when one expects competitors to choose quickly or is uncertain about how long competitors will search. Descriptively, we observe that competition drastically reduces information search prior to choice. {\textcopyright} 2014 Elsevier B.V.},
author = {Phillips, Nathaniel D. and Hertwig, Ralph and Kareev, Yaakov and Avrahami, Judith},
doi = {10.1016/j.cognition.2014.06.006},
file = {:Users/maraaverick/Dropbox/Mendeley Desktop/Phillips et al. - 2014 - Rivals in the dark How competition influences search in decisions under uncertainty.pdf:pdf},
isbn = {0010-0277},
issn = {18737838},
journal = {Cognition},
keywords = {Competition,Decisions from experience,Decisions under uncertainty,Information search},
number = {1},
pages = {104--119},
pmid = {25010397},
publisher = {Elsevier B.V.},
title = {{Rivals in the dark: How competition influences search in decisions under uncertainty}},
url = {http://dx.doi.org/10.1016/j.cognition.2014.06.006},
volume = {133},
year = {2014}
}
