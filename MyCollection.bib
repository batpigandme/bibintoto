journal = {Proceedings of the Annual Hawaii International Conference on System Sciences},
keywords = {EARL,STS},
mendeley-tags = {EARL,STS},
pages = {3938--3947},
title = {{Understanding the influence of social media in the workplace: An integration of media synchronicity and social capital theories}},
year = {2012}
