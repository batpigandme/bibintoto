isbn = {9781450321389},
issn = {9781450321389},
keywords = {Basketball,Deep Learning,Imitation Learning,Policy Plan- ning,Spatiotemporal Models},
title = {{Learning Long-term Planning in Basketball Using Hierarchical Memory Networks Stephan}},
year = {2016}
}
